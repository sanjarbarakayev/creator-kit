# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database (direct connection for migrations)
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Meta / Instagram OAuth
NUXT_META_APP_ID=your-meta-app-id
NUXT_META_APP_SECRET=your-meta-app-secret
NUXT_INSTAGRAM_REDIRECT_URI=http://localhost:3000/api/instagram/callback

# TikTok OAuth
NUXT_TIKTOK_CLIENT_KEY=your-tiktok-client-key
NUXT_TIKTOK_CLIENT_SECRET=your-tiktok-client-secret

# Token encryption (32-byte hex string)
NUXT_TOKEN_ENCRYPTION_KEY=your-64-char-hex-string

# Telegram Bot
NUXT_TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# App URL
NUXT_PUBLIC_APP_URL=http://localhost:3000
